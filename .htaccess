<IfModule mod_rewrite.c>
  RewriteEngine on
  
  #admin section
  RewriteRule 	^admin\/([a-z]+)$	admin/index.php?view=$1 [L]
  
  #opml
  RewriteRule 	^opml.*$	opml.php?action=Export [L]
  
  #item view
  RewriteCond  %{REQUEST_URI} !css\/
  RewriteCond  %{REQUEST_URI} !img\/
  RewriteCond  %{REQUEST_URI} !admin\/
  RewriteRule ^(.+)\/([0-9][0-9][0-9][0-9])\/([0-9][0-9]?)\/?([0-9]?[0-9])?\/?(.*)?$	feed.php?channel=$1&y=$2&m=$3&d=$4&iid=$5 [L]
  
  #feed view
  RewriteCond  %{REQUEST_URI} !css\/
  RewriteCond  %{REQUEST_URI} !img\/
  RewriteCond  %{REQUEST_URI} !magpierss-0.61\/
  RewriteCond  %{REQUEST_URI} !admin\/
  RewriteRule ^(.+)\/(.*)?$   feed.php?channel=$1&iid=$2  
</IfModule>
